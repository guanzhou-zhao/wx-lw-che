/**app.wxss**/
@import 'weui.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

page {
  background-color: #f8f8f8;
  font-size: 16px;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.page__hd {
  padding: 40px;
}

.page__bd {
  padding-bottom: 40px;
}

.page__bd_spacing {
  padding-left: 15px;
  padding-right: 15px;
}

.page__ft {
  padding-bottom: 10px;
  text-align: center;
}

.page__title {
  text-align: left;
  font-size: 20px;
  font-weight: 400;
}

.page__desc {
  margin-top: 5px;
  color: #888;
  text-align: left;
  font-size: 14px;
}

.my_button_default {
  color: #353535;
  border: 1px solid #353535;
  background-color: transparent;
  display: inline-block;
  line-height: 2.3;
  font-size: 13px;
  padding: 0 1.34emem;
  box-sizing: border-box;
  text-align: center;
  text-decoration: none;
  border-radius: 5px;
  overflow: hidden;
}

.my_button {
  display: inline-block;
  line-height: 1.8;
  font-size: 13px;
  padding: 0 1em;
  color: #fff;
  background-color: #1aad19;
  box-sizing: border-box;
  text-align: center;
  text-decoration: none;
  border-radius: 5px;
  overflow: hidden;
}
